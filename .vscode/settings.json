{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": [
        "source.organizeImports",
        "source.fixAll.eslint"
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "editor.tabSize": 4,
    "[css]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
}